<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ProgressView">
        <attr name="pv_progress_color" format="color" />
        <attr name="pv_track_color" format="color" />
        <attr name="pv_animate_progress_changes" format="boolean" />
        <attr name="pv_max_progress" format="float" />
    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="cpv_item_color" format="color" />
        <attr name="cpv_item_offset" format="dimension" />
        <attr name="item_side_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AnimCheckBox">
        <attr name="acb_border_color" format="color" />
        <attr name="acb_checked_color" format="color" />
        <attr name="acb_check_icon_color" format="color" />
        <attr name="acb_border_width" format="dimension" />
        <attr name="acb_check_icon_width" format="dimension" />
        <attr name="acb_checked" format="boolean" />
    </declare-styleable>
</resources>